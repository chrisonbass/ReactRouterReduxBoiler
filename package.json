    "browserify": "^16.2.3",
    "css-reset-and-normalize": "^1.2.0",
    "envify": "^4.1.0",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "fancy-log": "^1.3.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^4.0.0",
    "gulp-exit": "0.0.2",
